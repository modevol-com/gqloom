{
  "name": "@gqloom/mikro-orm",
  "version": "0.9.0",
  "description": "GQLoom integration with Mikro ORM",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "check:type": "tsc --noEmit"
  },
  "files": ["dist"],
  "keywords": ["gqloom", "graphql", "schema", "typescript", "mikro-orm"],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "@gqloom/core": ">= 0.9.0",
    "@mikro-orm/core": ">= 6.0.0",
    "graphql": ">= 16.8.0"
  },
  "devDependencies": {
    "@gqloom/core": "workspace:*",
    "@mikro-orm/core": "^6.4.6",
    "@mikro-orm/libsql": "^6.4.6"
  },
  "homepage": "https://gqloom.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/modevol-com/gqloom.git",
    "directory": "packages/mikro-orm"
  },
  "publishConfig": {
    "access": "public"
  }
}
