{
  "name": "@gqloom/mikro-orm",
  "version": "0.17.0-next.1",
  "description": "GQLoom integration with Mikro ORM",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "check:type": "tsgo --noEmit"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gqloom",
    "graphql",
    "schema",
    "typescript",
    "mikro-orm"
  ],
  "author": "xcfox",
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/sql": "^7.0.0-rc.0"
  },
  "peerDependencies": {
    "@gqloom/core": ">= 0.16.0",
    "@mikro-orm/core": ">= 7.0.0",
    "@mikro-orm/sql": ">= 7.0.0",
    "graphql": ">= 16.8.0"
  },
  "peerDependenciesMeta": {
    "@mikro-orm/sql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@gqloom/core": "workspace:*",
    "@gqloom/valibot": "workspace:*",
    "@mikro-orm/core": "^7.0.0-rc.0",
    "@mikro-orm/decorators": "^7.0.0-rc.0",
    "@mikro-orm/libsql": "^7.0.0-rc.0",
    "reflect-metadata": "^0.2.2",
    "valibot": "1.2.0"
  },
  "homepage": "https://gqloom.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/modevol-com/gqloom.git",
    "directory": "packages/mikro-orm"
  },
  "publishConfig": {
    "access": "public"
  }
}
