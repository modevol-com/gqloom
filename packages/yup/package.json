{
  "name": "@gqloom/yup",
  "version": "0.9.1",
  "description": "Create GraphQL schema and resolvers easily using using Yup!",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "check:type": "tsc --noEmit"
  },
  "files": ["dist"],
  "keywords": [
    "gqloom",
    "graphql",
    "schema",
    "typescript",
    "valibot",
    "validation",
    "validate"
  ],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "graphql": ">= 16.8.0",
    "@gqloom/core": ">= 0.9.0",
    "yup": ">= 1.0.0"
  },
  "devDependencies": {
    "yup": "^1.4.0",
    "@gqloom/core": "workspace:*"
  },
  "homepage": "https://gqloom.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/modevol-com/gqloom.git",
    "directory": "packages/yup"
  },
  "publishConfig": {
    "access": "public"
  }
}
