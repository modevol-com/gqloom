{
  "name": "@gqloom/prisma",
  "version": "0.11.0",
  "description": "GQLoom integration with Prisma",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./context": {
      "import": {
        "types": "./dist/context.d.ts",
        "default": "./dist/context.js"
      },
      "require": {
        "types": "./dist/context.d.cts",
        "default": "./dist/context.cjs"
      }
    },
    "./generated": {
      "import": {
        "types": "./generated/index.d.ts",
        "default": "./generated/index.js"
      },
      "require": {
        "types": "./generated/index.d.cts",
        "default": "./generated/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "check:type": "tsgo --noEmit",
    "init": "node scripts/init_tables.cjs",
    "generate": "pnpm build && prisma generate",
    "push": "pnpm build && pnpm generate && pnpm run init"
  },
  "bin": {
    "prisma-gqloom": "./bin/generator.cjs"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gqloom",
    "graphql",
    "schema",
    "typescript",
    "prisma"
  ],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "@gqloom/core": ">= 0.10.0",
    "graphql": ">= 16.8.0",
    "prisma": ">= 5.0.0"
  },
  "devDependencies": {
    "@gqloom/core": "workspace:*",
    "@gqloom/zod": "workspace:*",
    "@prisma/adapter-better-sqlite3": "^6.17.1",
    "@prisma/client": "^6.17.1",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "graphql-yoga": "^5.6.0",
    "prisma": "^6.17.1",
    "zod": "4.1.5"
  },
  "homepage": "https://gqloom.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/modevol-com/gqloom.git",
    "directory": "packages/prisma"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/generator-helper": "^6.17.1",
    "ts-morph": "^27.0.0"
  }
}
