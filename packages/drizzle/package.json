{
  "name": "@gqloom/drizzle",
  "version": "0.11.0",
  "description": "GQLoom integration with Drizzle ORM",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./context": {
      "import": {
        "types": "./dist/context.d.ts",
        "default": "./dist/context.js"
      },
      "require": {
        "types": "./dist/context.d.cts",
        "default": "./dist/context.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "check:type": "tsgo --noEmit",
    "push": "node scripts/push.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gqloom",
    "graphql",
    "schema",
    "typescript",
    "drizzle",
    "drizzle-orm"
  ],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "@gqloom/core": ">= 0.10.0",
    "drizzle-orm": ">= 0.44.0",
    "graphql": ">= 16.8.0"
  },
  "devDependencies": {
    "@gqloom/core": "workspace:*",
    "@gqloom/valibot": "workspace:*",
    "@libsql/client": "^0.15.15",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.4",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.45.1",
    "mysql2": "^3.16.0",
    "pg": "^8.18.0",
    "tsx": "^4.21.0",
    "valibot": "1.2.0"
  },
  "homepage": "https://gqloom.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/modevol-com/gqloom.git",
    "directory": "packages/drizzle"
  },
  "publishConfig": {
    "access": "public"
  }
}
